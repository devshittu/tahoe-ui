{
  "name": "@tahoe-ui/layout",
  "version": "0.0.1",
  "description": "Layout primitives for Tahoe UI design system",
  "keywords": [
    "react",
    "layout",
    "components",
    "flexbox",
    "grid",
    "box",
    "design-system"
  ],
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./box": {
      "import": {
        "types": "./dist/box/index.d.ts",
        "default": "./dist/box/index.js"
      },
      "require": {
        "types": "./dist/box/index.d.cts",
        "default": "./dist/box/index.cjs"
      }
    },
    "./container": {
      "import": {
        "types": "./dist/container/index.d.ts",
        "default": "./dist/container/index.js"
      },
      "require": {
        "types": "./dist/container/index.d.cts",
        "default": "./dist/container/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src/",
    "types:check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@tahoe-ui/core": "workspace:*"
  },
  "devDependencies": {
    "@tahoe-ui/tsconfig": "workspace:*",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tsup": "^8.3.5",
    "typescript": "^5.9.0-beta"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
