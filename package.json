{
  "name": "tahoe-ui",
  "version": "0.0.0",
  "description": "Futuristic UI library for React",
  "license": "MIT",
  "repository": "https://github.com/devshittu/tahoe-ui",
  "author": "Muhammed Shittu <mshittu.work@gmail.com>",
  "maintainers": [
    "Muhammed Shittu <devshittu@gmail.com>"
  ],
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "turbo clean && rm -rf node_modules",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo build --filter=./packages/* && changeset publish",
    "playground:dev": "turbo dev --filter=playground",
    "playground:build": "turbo build --filter=playground",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o storybook-static",
    "postbuild-storybook": "mv storybook-static public/storybook",
    "build-storybook-docs": "storybook build --docs",
    "build:vercel": "turbo build --filter=playground && yarn build-storybook",
    "prepare": "husky"
  },
  "dependencies": {
    "@codesandbox/sandpack-react": "^2.20.0",
    "@floating-ui/react": "^0.27.16",
    "@headlessui/react": "^2.2.7",
    "@monaco-editor/react": "^4.7.0",
    "@tahoe-ui/tailwind-preset": "workspace:*",
    "@tahoe-ui/theme": "workspace:*",
    "@tailwindcss/postcss": "^4.0.0",
    "@tanstack/react-query": "^5.62.8",
    "@tanstack/react-query-devtools": "^5.62.8",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "axios": "^1.7.9",
    "clsx": "^2.1.1",
    "framer-motion": "^11.15.0",
    "mitt": "^3.0.1",
    "next": "15.0.5",
    "next-themes": "^0.4.6",
    "prismjs": "^1.29.0",
    "react": "19.0.1",
    "react-dom": "19.0.1",
    "react-icons": "^5.4.0",
    "react-intersection-observer": "^9.14.0",
    "shiki": "^3.21.0",
    "tailwind-merge": "^2.5.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@chromatic-com/storybook": "^3.2.2",
    "@storybook/addon-essentials": "8.4.2",
    "@storybook/addon-interactions": "8.4.2",
    "@storybook/addon-onboarding": "8.4.2",
    "@storybook/blocks": "8.4.2",
    "@storybook/nextjs": "8.4.2",
    "@storybook/react": "8.4.2",
    "@storybook/test": "8.4.2",
    "@tanstack/eslint-plugin-query": "^5.62.1",
    "@types/node": "^20",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "15.0.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-storybook": "^0.11.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "storybook": "8.4.2",
    "tailwindcss": "^3.4.1",
    "turbo": "^2.3.0",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "react",
    "ui",
    "design",
    "component",
    "library",
    "tailwindcss",
    "typescript",
    "nextjs"
  ]
}
