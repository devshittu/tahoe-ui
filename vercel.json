{
  "rewrites": [
    {
      "source": "/storybook",
      "destination": "/storybook/index.html"
    },
    {
      "source": "/storybook/:path*",
      "destination": "/storybook/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/storybook/:path*",
      "destination": "/storybook/:path*",
      "statusCode": 200
    }
  ],
  "buildCommand": "npm run build:vercel",

  "outputDirectory": ".vercel/output",

  "framework": "nextjs"
}
