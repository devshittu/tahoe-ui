{
  "rewrites": [
    {
      "source": "/storybook",
      "destination": "/storybook/index.html"
    },
    {
      "source": "/storybook/:path*",
      "destination": "/storybook/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/storybook/*",
      "destination": "/storybook/:splat",
      "statusCode": 200
    }
  ]
}
